msgctxt "5bd482de268e4ee7882d52c39a7267ac"
msgid "༄༅། ། དང་པོ ། ༄། ལྷན་ གཅིག་ སྐྱེས་ སྦྱོར་ ཞེས་ བྱ་བ་ བཞུགས་ སོ །"
msgstr "The text called co-arising union"

msgctxt "eb8df20164804503b60e6dcd24f61289"
msgid "བླ་མ་ རིན་པོ་ཆེ་ རྣམས་ ལ་ ཕྱག་ འཚལ་ ལོ ། །"
msgstr "I prosterate to the precious master"

msgctxt "71586aad6e244096974465f819d1ac53"
msgid "རང་བཞིན་ དབྱིངས་ ལས་མ་ གཡོས་ ཀྱང་ ། །"
msgstr "Though, not fluctuating from the natural expanse,"

msgctxt "a65996f6cd4a4086a3d7f63b01f23f66"
msgid "ཐུགས་རྗེ␣ས་ འགྲོ་བ␣འི་ དོན་ མཛད་ ཕྱིར ། །"
msgstr "acting for the sentient beings through compassion"

msgctxt "2e707bee2c4e4687878336f8210f1325"
msgid "སྤྲུལ་སྐུ་ འཇའ་ཚོན་ ལྟ་བུ␣ར་ བྱོན ། །"
msgstr "Emanation body arrives like the colors of rainbow"

msgctxt "f6096b07243b4a85b466c9b01b153227"
msgid "ཨུ་རྒྱན་ ཕག་མོ་ ལ་ ཕྱག་ འཚལ ། །"
msgstr "(I) prostrate to varahi of uddiyana."

msgctxt "d0edba708c1c450daca0fbe257167355"
msgid "ཕག་མོ␣ས་ དོངས་ སུ་ བྱིན་ བརླབས་པ␣འི ། །"
msgstr "Blessed personally by the varahi"

msgctxt "fc8afd9d22134a80af0e4296fa26c50e"
msgid "ཐུན་ གཅིག་ བསྒོམ་ ནས་ དངོས་གྲུབ་ ཐོབ ། །"
msgstr "Thus, accomplishment is received by mediating for one session"

msgctxt "74f716fc77bf470d9176021cedae7d6c"
msgid "མཁའ་འགྲོ་ དབང་ དུ་ སྡུད་ མཛད་པ␣འི ། །"
msgstr "The (one) submitting Dakini under power,"

msgctxt "1d587f36575c4cfc99042c997487d29a"
msgid "ལྭ་བ་པ་ ལ་ ཕྱག་ འཚལ་ བསྟོད ། །"
msgstr "(I) prostrate in praise to Lvawapa."

msgctxt "e4c56bc530c1466d8d69729b48f20c26"
msgid "དེ་ ཡི་ བྱིན་རླབས་ རཀྵི་ ཏ ། །"
msgstr "Rakshita is its blessing,"

msgctxt "df43fbdc4d9548be859e4d5c433d9e7a"
msgid "དེེ་ ཡི་ བརྒྱུད་འཛིན་ ལྷོ་བྲག་པ ། །"
msgstr "Lhodrakpa is its linage holder"

msgctxt "78ee6bd75774468c8a22135e0e29a9a8"
msgid "དེ་ ཡིི་ བྱིན་རླབས་ གུང་ ཐང་པ ། །"
msgstr "Gungthang pa is its blessing,"

msgctxt "889e244fd72548268b41f4ef45bccd91"
msgid "མི་ལ␣འི་ ཐུགས་ ཀྱི་ སྲས་ གྱུར་པ ། །"
msgstr "becoming the heart disciple of Milarepa."

msgctxt "10aa66bd1a5c4627b97faa845220412c"
msgid "རྗེ་ མཚན་ལྡན་ བླ་མ་ རིན་པོ་ཆེ ། །"
msgstr "oh revered precious qualified master"

msgctxt "63a4949bb8da40c48e59a5d0df42a969"
msgid "བརྒྱུད་པ␣འི་ བླ་མ་ རྣམས་ ལ་ བདག་ ཕྱག་ འཚལ ། །"
msgstr "I prostrate to the linage masters."

msgctxt "504a607101db49d0b3faee19958c1876"
msgid "བདག་ཉིད་ བརྗེད་པ␣ས་ འཇིགས་པ་ དང་ ། །"
msgstr "Fear of forgetting the great one"

msgctxt "81930031098a43588ec89ecd842a5e06"
msgid "སྐལ་ལྡན་ འགའ་ ལ་ ཕན་ གདགས་ ཕྱིར ། །"
msgstr "And to benefit some fortunate ones"

msgctxt "176754b16cbb478eb5e88fe61c9d203c"
msgid "ལྟ་ རྗེའི་ གསུང་སྒྲོས་ ཇི་བཞིན་ དུ ། །"
msgstr "in the manner of statement of the masters of the view"

msgctxt "2be51189cb77449db0fa54bc19d4bec6"
msgid "སྐྱེས་ སྦྱོར་ གདམས་ངག་ བྲི་བ␣ར་ བྱ ། །"
msgstr "have written the oral instruction of co-emergent union. "

msgctxt "646465da984047c48cf696e72fd288e6"
msgid "དེ་ཡང་ སྐྱེས་ སྦྱོར་ གདམས་ངག་ ལ ། །"
msgstr "Moreover, on the oral instruction of co-emergent union,"

msgctxt "c3b4e85fd929435aa2cf3d108cd014a5"
msgid "རྗེེའི་ ཞལ་ ནས་ འདི་སྐད་ གསུང་ ། །"
msgstr "The revered one state as this."

msgctxt "ffa5e97f87ec4662b80598ac8a9a642e"
msgid "མཁས་ བཙུན་ སྒོམ་ སྟོན་ ཐམས་ཅད་ ཀྱིས ། །"
msgstr "all the scholars and the teachers of cultivation."

msgctxt "8c15a573421f442f881a958b45d4909f"
msgid "ཆོས་ སུ་ འདི་ཉིད་ ཟབ་བོ་ གསུང་ ། །"
msgstr "has said this profoundly in dharma"

msgctxt "1db99c595aad45fbaef4184041eea9f1"
msgid "རྣམ་རྟོག་ དང་ ནི་ ཉོན་མོངས་པ ། །"
msgstr "Afflictions and thoughts"

msgctxt "dd7972ba454d486cb60bdf961126741d"
msgid "ཐམས་ཅད་ འདུལ་བ␣ས་ འདུལ་བ་ ཡིན ། །"
msgstr "being subdued is the vinaya"

msgctxt "3215d27652ee4320a7bc349db254ff6a"
msgid "རྣམ་པ␣ར་ རྟོག་པ་ ཆོས་སྐུ་ རུ ། །"
msgstr "Thoughts as the dharma body."

msgctxt "604fdba29fa74119be765082bf9a77c4"
msgid "ངེས་པ␣འི་ ཤེས་པ་ བསྐྱེད་པ␣འི་ ས ། །"
msgstr "The ground of arising mind of certainty"

msgctxt "e31f7c4c4bc347d3a2a0927a7e25590e"
msgid "སྒྲོ་འདོགས་ ནང་ དུ་ གཅོད་པ␣ས་ ན ། །"
msgstr "imputation of sound being cut within. "

msgctxt "376cf3514fd748749069b69773022e22"
msgid "འདི་ ནི་ ཚད་མ␣འི་ གདམས་ངག་ ཡིན ། །"
msgstr "It is the oral instruction of valid cognition."

msgctxt "59ba3ed44e0e4f09b361f8a6a05473f5"
msgid "རྟག་ཆད་ སྒྲོ་སྐུར་ ལ་སོགས་པ␣འི ། །"
msgstr "Of superimposition and depreciation of permanent and nihilist view "

msgctxt "fcd57f0e97ca4f35ab4f1be8e80407e1"
msgid "མཐའ་ དང་ བྲལ་བ␣ས་ དབུ་མ་ ཡིན ། །"
msgstr "Is the middle way separate from extremes"

msgctxt "a8558cb6257143c18d9e63ec17b0ac99"
msgid "སྨྲ་ བསམ་བརྗོད་ དུ་ མེད་པ␣འི་ ཕྱིར ། །"
msgstr "Because it is beyond statement and contemplation,"

msgctxt "cd0205199f4a4992be0c2611965040f2"
msgid "ཕ་རོལ་ ཕྱིན་པ␣འི་ གདམས་ངག་ ཡིན ། །"
msgstr "(It's) the oral instruction of going beyond."

msgctxt "a831597d58e34c898af1fa2751eb6eda"
msgid "འཁོར་ དང་ མྱ་ངན་ འདས་པ␣འི་ ཆོས ། །"
msgstr "Dharma beyond nirvana and samsara."

msgctxt "465440f1f0f74d57973d712fa71801c8"
msgid "སེམས་ སུ་ རྫོགས་པ␣ས་ རྫོགས་ཆེན་ ཡིན ། །"
msgstr "Since it completes in mind, it is the great completion "

msgctxt "89d56b1b905b4e1399426d9c887369ab"
msgid "བཟང་ རྟོག་ ངན་ རྟོག་ ཐམས་ཅད་ ཀུན ། །"
msgstr "All the good and bad thoughts,"

msgctxt "5beb8aa185cc494281e75751ce7105f5"
msgid "ཡིད་ ལ་ མི་ བྱེད་ ཕྱག་རྒྱ་ ཆེ ། །"
msgstr "Not taken into mind is Mahamudra."

msgctxt "f4cdb46ee9f048a992530a159d628fae"
msgid "སྡུག་བསྔལ་ ཞི་བ␣ས་ ཞི་བྱེད་ ཡིན ། །"
msgstr "Since it eases suffering, it is ease."

msgctxt "bb5133cb0f954428a625826f5c5a7736"
msgid "ཉོན་མོངས་ དང་ ནི་ རྣམ་རྟོག་ ཀུན ། །"
msgstr "All the thoughts and afflicted emotions,"

msgctxt "9b6dd469c4e64a4aa43f2b19c6b5a22a"
msgid "ལམ་ དུ་ སློངས་པ␣ས་ གསང་སྔགས་ ཡིན ། །"
msgstr "By taking up as path is the Tantra."

msgctxt "dee9f30e877a49cb9c11a6e217e2563c"
msgid "སེམས་ དང་ རྣམ་རྟོག་ ཆོས་སྐུ་ གསུམ ། །"
msgstr "The three; Dharma body, thoughts, and mind,"

msgctxt "2a0367f757fb4afc94d4eddd34ef68b3"
msgid "དང་པོ་ ལྷན་ཅིག་ སྐྱེས་པ་ དེ ། །"
msgstr "first, when it co-arises,"

msgctxt "4d0f761360cf4342be703e92f254cc02"
msgid "གདམས་པ␣ས་ སེམས་ སུ་ སྦྱོར་བ␣འི་ ཕྱིར ། །"
msgstr "Because of the union in mind by instruction,"

msgctxt "6f531410cf034beeaaf25113567b0657"
msgid "ལྷན་ཅིག་ སྐྱེས་ སྦྱོར་ ཞེས་ སུ་ བཤད ། །"
msgstr "It is called the co-arising union."

msgctxt "0451bfb3ac9241beacc450c4acc89b82"
msgid "འདྲེ་ དང་ བདུད་ ལ་སོགས་པ་ ཡི ། །"
msgstr "Of the evil spirits and demons"

msgctxt "33557b8f7ead482691a660bca7aacf8b"
msgid "བར་ཆད་ ལ་ ཡང་ སྔགས་ སོ་ གསུང་ ། །"
msgstr "Praise the obstacles too"

msgctxt "daf0dc9419b54d0283eb5735884b08c4"
msgid "འདི་ ལ་ བསྡུས་པ␣འི་ དོན་ དྲུག་ སྟེ ། །"
msgstr "It contains six meanings,"

msgctxt "c66261c323e44e7593c448b1ad11cda5"
msgid "བླ་མ་ དང་ ནི་ སློབ་མ་ དང་ ། །"
msgstr "Master and the student"

msgctxt "da51b4adbf694548af4aa4910b89b62e"
msgid "གོ་ཆ་ བགོ་ དང་ ཤེས་རབ་ གསུམ ། །"
msgstr "Three; wisdom, division, and armor,"

msgctxt "173fe70a5dc447ef81f781b91d959ea7"
msgid "ཉམས་ དང་ ཕན་ཡོན་ འབྲས་་བུ་ སྟེ ། །"
msgstr "The result, benefit and practice,"

msgctxt "619672bd3cc0427885c1a19a8a50b3ee"
msgid "འདི་ དོན་ རྣམ་པ་ དྲུག་ གིས་ བཤད ། །"
msgstr "Six types explain their meaning."

msgctxt "48e1145e708647658db8ba1165f0db8c"
msgid "དང་ པོོ་ སློབ་མ␣འི་ མཚན་ཉིད་ ནི ། །"
msgstr "First, the characteristics of student"

msgctxt "4cf5500b526b4fd3b1651bbcc405efad"
msgid "རྣམ་པ་ གཉིས་ སུ་ རྗེ་བཙུན་ གསུང་ ། །"
msgstr "(Jetsun) states it in two ways."

msgctxt "005a5ce3668643c2b2814e4e99fa8cec"
msgid "སྤྲང་ བྱ་ དང་ ནི་ བླང་ བྱ␣འོ ། །"
msgstr "Those of which are to be abandoned and to be adopted."

msgctxt "2233ef87c5294982a0e0c6e249aa3c49"
msgid "སྤང་བྱ་ ལ་ ཡང་ རྣམ་པ་ བཞི ། །"
msgstr "Those to be abandoned are too of four types."

msgctxt "e4ecacd95a884cd5ab700cbc37ab4fc9"
msgid "རྒྱུ་ ཡི་ དུས་ ན་ ཞེན་པ་ དང་ ། །"
msgstr "Clinging at the time of cause"

msgctxt "b95689fbbafb4aa0b26d4c6c4d1846aa"
msgid "བདག་པོ␣འི་ དུས་ ན་ ལོག་ བཤེསཟིན ། །"
msgstr "And misconception grasping at the time of possessor,"

msgctxt "d5fb73e8a47347c1b77ce8e1eae44ca5"
msgid "དམིགས་པ␣འི་ དུས་ ན་ འཁྲུལ་པ་ དང་ ། །"
msgstr "Illusion at the time of visualization,"

msgctxt "c7518e15023d420084d0a1d46564af5a"
msgid "དེ་ མ་ ཐག་པ␣འི་ དུས་ སུ་ ནི ། །"
msgstr "And at the immediate time,"

msgctxt "bc5dcc1600084c1cb6d3e6c02c55fc3e"
msgid "རེ་དོགས་ གཉིས་ སུ་ ལྷུང་བ␣འོ ། །"
msgstr "Falls into both doubt and expectation."

msgctxt "4aab80f100dd4699b7e1610f6ffe6c23"
msgid "དང་པོ་ རྒྱུ་དུས་ ཞེན་པ་ ནི ། །"
msgstr "First clinging at the time of cause,"

msgctxt "3383a9e9e29a46c3b62841b46b470ae1"
msgid "སྣང་སྲིད་ སྣ་ཚོགས་ སྣང་བ་ འདི ། །"
msgstr "This appearance of various possible appearance,"

msgctxt "f75458be7ed943e08ac3e0120631d6cc"
msgid "རྨི་ལམ་ དག་ གི་ ནོར་ ཟས་ བཞིན ། །"
msgstr "Like the food and wealth of the dream itself,"

msgctxt "ece65909f02544a9ad9dadef146e51e1"
msgid "སྣངབ་ ཉིད་ ནི་ སྟོང་པ་ ལ ། །"
msgstr "The appearance itself is empty,"

msgctxt "bc50893794314b91b4cce782ba0e3697"
